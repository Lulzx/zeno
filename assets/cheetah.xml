<mujoco model="half_cheetah">
    <option timestep="0.01" gravity="0 0 -9.81"/>

    <default>
        <joint armature="0.1" damping="0.5" limited="true"/>
        <geom contype="1" conaffinity="1" condim="3" friction="0.8 0.1 0.1" margin="0.01"/>
    </default>

    <worldbody>
        <!-- Ground plane -->
        <geom type="plane" size="50 50 0.1" rgba="0.8 0.9 0.8 1"/>

        <!-- Torso -->
        <body name="torso" pos="0 0 0.7">
            <joint name="root" type="free"/>
            <geom name="torso_geom" type="capsule" fromto="-0.5 0 0 0.5 0 0" size="0.046" rgba="0.9 0.6 0.3 1"/>

            <!-- Back thigh -->
            <body name="back_thigh" pos="-0.5 0 0">
                <joint name="back_hip" type="hinge" axis="0 1 0" range="-50 90"/>
                <geom type="capsule" fromto="0 0 0 0.16 0 -0.13" size="0.046" rgba="0.9 0.6 0.3 1"/>

                <!-- Back shin -->
                <body name="back_shin" pos="0.16 0 -0.13">
                    <joint name="back_knee" type="hinge" axis="0 1 0" range="-150 0"/>
                    <geom type="capsule" fromto="0 0 0 0 0 -0.19" size="0.046" rgba="0.9 0.6 0.3 1"/>

                    <!-- Back foot -->
                    <body name="back_foot" pos="0 0 -0.19">
                        <joint name="back_ankle" type="hinge" axis="0 1 0" range="-50 50"/>
                        <geom type="capsule" fromto="0 0 0 0.1 0 0" size="0.046" rgba="0.6 0.4 0.2 1"/>
                    </body>
                </body>
            </body>

            <!-- Front thigh -->
            <body name="front_thigh" pos="0.5 0 0">
                <joint name="front_hip" type="hinge" axis="0 1 0" range="-50 90"/>
                <geom type="capsule" fromto="0 0 0 -0.14 0 -0.13" size="0.046" rgba="0.9 0.6 0.3 1"/>

                <!-- Front shin -->
                <body name="front_shin" pos="-0.14 0 -0.13">
                    <joint name="front_knee" type="hinge" axis="0 1 0" range="-150 0"/>
                    <geom type="capsule" fromto="0 0 0 0.07 0 -0.15" size="0.046" rgba="0.9 0.6 0.3 1"/>

                    <!-- Front foot -->
                    <body name="front_foot" pos="0.07 0 -0.15">
                        <joint name="front_ankle" type="hinge" axis="0 1 0" range="-50 50"/>
                        <geom type="capsule" fromto="0 0 0 0.1 0 0" size="0.046" rgba="0.6 0.4 0.2 1"/>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <motor joint="back_hip" ctrlrange="-1 1" gear="120"/>
        <motor joint="back_knee" ctrlrange="-1 1" gear="90"/>
        <motor joint="back_ankle" ctrlrange="-1 1" gear="60"/>
        <motor joint="front_hip" ctrlrange="-1 1" gear="120"/>
        <motor joint="front_knee" ctrlrange="-1 1" gear="60"/>
        <motor joint="front_ankle" ctrlrange="-1 1" gear="30"/>
    </actuator>

    <sensor>
        <!-- Torso sensors -->
        <framepos objtype="body" objname="torso"/>
        <framequat objtype="body" objname="torso"/>
        <framelinvel objtype="body" objname="torso"/>
        <frameangvel objtype="body" objname="torso"/>

        <!-- Joint position sensors -->
        <jointpos joint="back_hip"/>
        <jointpos joint="back_knee"/>
        <jointpos joint="back_ankle"/>
        <jointpos joint="front_hip"/>
        <jointpos joint="front_knee"/>
        <jointpos joint="front_ankle"/>

        <!-- Joint velocity sensors -->
        <jointvel joint="back_hip"/>
        <jointvel joint="back_knee"/>
        <jointvel joint="back_ankle"/>
        <jointvel joint="front_hip"/>
        <jointvel joint="front_knee"/>
        <jointvel joint="front_ankle"/>
    </sensor>
</mujoco>
